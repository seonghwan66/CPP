----2743----

#include <iostream>
#include <string>
using namespace std;

int main() {
	string s;
	cin >> s;

	cout << s.length() << endl;
	return 0;
}

----9086----

#include <iostream>
#include <string>
using namespace std;

int main() {
	int T;
	string s;
	cin >> T;

	while (T--) {
		cin >> s;
		cout << s[0] << s[s.length() - 1] << endl;
	}
	return 0;
}

----11654----

#include <iostream>
using namespace std;

int main() {
	char s;
	cin >> s;

	cout << (int)s << endl;

	return 0;
}
